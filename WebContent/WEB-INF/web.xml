<?xml version="1.0" encoding="UTF-8"?>
<web-app>
<servlet>
	<servlet-name>LoginServlet</servlet-name>
	<servlet-class>test.LoginServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>LoginServlet</servlet-name>
	<url-pattern>/login</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>RegServlet</servlet-name>
	<servlet-class>test.RegServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>RegServlet</servlet-name>
	<url-pattern>/Register</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>RetrieveTrainServlet</servlet-name>
	<servlet-class>test.RetrieveTrainServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>RetrieveTrainServlet</servlet-name>
	<url-pattern>/retrieveTrain</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>AddTrainServlet</servlet-name>
	<servlet-class>test.AddTrainServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>AddTrainServlet</servlet-name>
	<url-pattern>/addTrain</url-pattern>
</servlet-mapping>

<welcome-file-list>
	<welcome-file>login.html</welcome-file>
</welcome-file-list>
</web-app>